include(FetchContent)

FetchContent_Declare(
  folly
  URL https://github.com/facebook/folly/archive/d59f9846848d6cb18fe714fd2d284ba23e1e3747.zip
  DOWNLOAD_EXTRACT_TIMESTAMP true
)

FetchContent_GetProperties(folly)

if(NOT folly_POPULATED)
  FetchContent_Populate(folly)

  # adds the targets: folly
  add_subdirectory(
    ${folly_SOURCE_DIR}
    ${folly_BINARY_DIR}
    )
endif()
